<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GraphicsBenchmarkActivity">

    <!-- Add UI elements specific to Graphics Benchmark Activity -->
    <TextView
        android:id="@+id/graphicsBenchmarkTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp"
        android:text="Graphics Benchmark Activity"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/graphicsBenchmarkResultTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/graphicsBenchmarkTitle"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:textSize="18sp" />

    <!-- Add any other UI elements and layout as needed for graphics benchmark activity -->
</RelativeLayout>
